version 42
__lua__
#include util.lua
#include load.lua
#include spell.lua
#include enemy.lua
#include player.lua
#include select_team.lua
#include battle.lua

function _init()
  reload(0x8000, 0, 0x2000, "data1.p8")

  local players = {
    warrior_new("vlad"),
    elf_new("lena"),
    --wizard_new("merl")
    priestess_new("aria")
  }

  local enemies = {
    "hobgoblin",
    "zombie",
    "witch",
    "wolf"
  }

  --battle = load_battle(players, enemies)
  select_team = load_select_team()
end

function _update()
  --update_battle(battle)
  update_select_team(select_team)
end

function _draw()
  cls()
  --map()
  --draw_battle(battle)
  draw_select_team(select_team)
end

